# perf diff config for NX651J
# audio
CONFIG_SND_SOC_MAX98937=y

# charger
CONFIG_NUBIA_CHARGE_FEATURE=y
CONFIG_NUBIA_POWEROFFCHARGE_NODE=y

# fingerprint
CONFIG_NUBIA_FINGERPRINT=y
CONFIG_NUBIA_FP_GOODIX=y
CONFIG_NUBIA_FP_GOODIX_GW9558=y

# fs
CONFIG_NUBIA_F2FS_TRIM_STAT=y
CONFIG_EXFAT_FS=y
CONFIG_EXFAT_DEFAULT_IOCHARSET="utf8"
CONFIG_EXFAT_VIRTUAL_XATTR=y
CONFIG_EXFAT_VIRTUAL_XATTR_SELINUX_LABEL="u:object_r:exfat:s0"

# lcd
CONFIG_NUBIA_DISP_PREFERENCE=y

# leds
CONFIG_ZTEMT_BREATH_LEDS=y

# leds - mulitcolor light
CONFIG_NUBIA_MULTICOLORLIGHT=y

# nfc
CONFIG_NFC=y
CONFIG_SEC_NFC=y
CONFIG_SEC_NFC_PRODUCT_N5=y
CONFIG_SEC_NFC_IF_I2C=y
# CONFIG_SEC_NFC_GPIO_CLK is not set
CONFIG_SEC_NFC_PMIC_LDO
CONFIG_SEC_NFC_DEDICATED_CLK

# sensors
CONFIG_NUBIA_SENSORS=y
CONFIG_NUBIA_SENSORS_STK3337=y

# touchscreen
CONFIG_NUBIA_DRIVERS=y
CONFIG_NUBIA_TOUCH=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_CORE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_I2C=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_RMI_DEV=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_FW_UPDATE=y
CONFIG_TOUCHSCREEN_SYNAPTICS_DSX_TEST_REPORTING=y

# usb
# CONFIG_NUBIA_USB_DISABLE_LINK_PM is not set
CONFIG_NUBIA_USB_CC_CTRL=y

# vibrator
CONFIG_NUBIA_HAPTIC_VIBRATOR=y

# others
CONFIG_NUBIA_CGF_NOTIFY_EVENT=y
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set

# additions - platform
CONFIG_MACH_NUBIA_SM7250=y
CONFIG_MACH_NUBIA_NX651J=y
